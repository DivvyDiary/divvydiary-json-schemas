{
  "$id": "https://divvydiary.com/schemas/symbol.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "maxLength": 255
    },
    "symbol": {
      "type": "string",
      "minLength": 1,
      "maxLength": 6
    },
    "isin": { "$ref": "defs.json#/definitions/isin" },
    "wkn": {
      "oneOf": [{ "$ref": "defs.json#/definitions/wkn" }, { "type": "null" }]
    },
    "exchange": { "$ref": "defs.json#/definitions/exchange" }
  }
}
